    "babel-plugin-transform-exponentiation-operator": "^6.22.0",
    "babel-plugin-transform-regenerator": "^6.22.0",
    "browserslist": "^2.1.2",
    "invariant": "^2.2.2",
    "semver": "^5.3.0"
